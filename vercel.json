{
    "version": 2,
    "rewrites": [
        {
          "source": "/api/(.*)",
          "destination": "https://backend-task-manager-pi.vercel.app/api/$1"
        }
      ],    
    "builds": [
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
    }
    ]
  }
  